{
  "page_label": "User Form",
  "fields": [
    {
      "field_id": "name",
      "field_label": "Name",
      "field_mandatory": "yes",
      "field_placeholder": "Enter Name",
      "field_type": "text",
      "field_regex": "/^[a-zA-Z ]+$/",
      "field_value": ""
    },
    {
      "field_id": "email",
      "field_label": "Email",
      "field_mandatory": "yes",
      "field_placeholder": "Enter Email",
      "field_regex": "/^([w-]+(?:.[w-]+)*)@((?:[w-]+.)*w[w-]{0,66}).([a-z]{2,6}(?:.[a-z]{2})?)$/i",
      "field_type": "text"
    },
    {
      "field_id": "contact",
      "field_label": "Contact No.",
      "field_mandatory": "yes",
      "field_placeholder": "Enter contact no.",
      "field_regex": "/^([0|+[0-9]{1,5})?([7-9][0-9]{9})$/",
      "field_type": "text"
    },
    {
      "field_id": "employment",
      "field_label": "Employment desired",
      "field_value": "Part-Time",
      "field_mandatory": "yes",
      "field_options": [
        {
          "option_label": "Full-Time"
        },
        {
          "option_label": "Part-Time"
        }
      ],
      "field_type": "select"
    }
  ]
}
